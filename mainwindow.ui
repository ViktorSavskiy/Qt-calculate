      <property name="styleSheet">
       <string notr="true">/* Стиль для экрана результата */
QLabel {
    background-color: #0a0a0a;
    border: 4px solid #2c3e50;
    border-radius: 8px;
    color: #32cd32; /* Более мягкий зеленый */
    font-family: &quot;Digital-7&quot;, &quot;Courier New&quot;, monospace;
    font-size: 52px;
    font-weight: bold;
    padding: 12px 20px;
    text-align: right;
    min-height: 70px;
    max-height: 70px;
    
    /* Для выравнивания текста */
    qproperty-alignment: 'AlignRight | AlignVCenter';
    qproperty-wordWrap: false;
}

/* Стиль при отображении чисел */
QLabel[text=&quot;0&quot;],
QLabel[text=&quot;1&quot;],
QLabel[text=&quot;2&quot;],
QLabel[text=&quot;3&quot;],
QLabel[text=&quot;4&quot;],
QLabel[text=&quot;5&quot;],
