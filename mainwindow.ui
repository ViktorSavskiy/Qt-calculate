    <item row="1" column="0">
     <widget class="QPushButton" name="pushButton_ac">
      <property name="styleSheet">
       <string notr="true">/* Стиль для главного окна */
MainWindow {
    background-color: #2c3e50;
}

/* Стиль для QLineEdit (экран калькулятора) */
QLineEdit {
    background-color: #1a1a1a;
    border: 3px solid #34495e;
    border-radius: 10px;
    color: #00ff00; /* Зеленый цвет как на старых калькуляторах */
    font-family: &quot;Digital-7&quot;, &quot;Courier New&quot;, monospace;
    font-size: 24px;
    font-weight: bold;
    padding: 10px 15px;
    text-align: right;
    selection-background-color: #34495e;
    min-height: 50px;
}

/* Эффект при наведении на экран */
QLineEdit:hover {
    border-color: #3498db;
}

/* Стиль когда экран в фокусе */
QLineEdit:focus {
    border-color: #e74c3c;
}

/* Стиль для кнопок калькулятора */
QPushButton {
    background-color: #34495e;
    border: 2px solid #2c3e50;
    border-radius: 8px;
    color: white;
    font-family: &quot;Arial&quot;, sans-serif;
    font-size: 16px;
    font-weight: bold;
    min-width: 40px;
    min-height: 40px;
}

/* Стиль при наведении на кнопку */
QPushButton:hover {
    background-color: #4a6b8a;
    border-color: #3498db;
}

/* Стиль при нажатии кнопки */
QPushButton:pressed {
    background-color: #2980b9;
    border-color: #e74c3c;
}

/* Специальные стили для операторов */
QPushButton[text=&quot;+&quot;],
QPushButton[text=&quot;-&quot;],
QPushButton[text=&quot;*&quot;],
QPushButton[text=&quot;/&quot;] {
    background-color: #e67e22;
    color: white;
}

QPushButton[text=&quot;+&quot;]:hover,
QPushButton[text=&quot;-&quot;]:hover,
QPushButton[text=&quot;*&quot;]:hover,
QPushButton[text=&quot;/&quot;]:hover {
    background-color: #d35400;
}

/* Стиль для кнопки равно */
QPushButton[text=&quot;=&quot;] {
    background-color: #27ae60;
    color: white;
}

QPushButton[text=&quot;=&quot;]:hover {
    background-color: #229954;
}

/* Стиль для кнопок очистки */
QPushButton[text=&quot;C&quot;],
QPushButton[text=&quot;AC&quot;] {
    background-color: #e74c3c;
    color: white;
}

QPushButton[text=&quot;C&quot;]:hover,
QPushButton[text=&quot;AC&quot;]:hover {
    background-color: #c0392b;
}

/* Стиль для цифровых кнопок */
QPushButton[text=&quot;0&quot;],
QPushButton[text=&quot;1&quot;],
QPushButton[text=&quot;2&quot;],
QPushButton[text=&quot;3&quot;],
QPushButton[text=&quot;4&quot;],
QPushButton[text=&quot;5&quot;],
QPushButton[text=&quot;6&quot;],
QPushButton[text=&quot;7&quot;],
QPushButton[text=&quot;8&quot;],
QPushButton[text=&quot;9&quot;] {
    background-color: #95a5a6;
    color: #2c3e50;
}

QPushButton[text=&quot;0&quot;]:hover,
QPushButton[text=&quot;1&quot;]:hover,
QPushButton[text=&quot;2&quot;]:hover,
QPushButton[text=&quot;3&quot;]:hover,
QPushButton[text=&quot;4&quot;]:hover,
QPushButton[text=&quot;5&quot;]:hover,
